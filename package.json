{
  "name": "Freelancer",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "cross-env ENVFILE=.env react-native run-android",
    "android-www": "cross-env ENVFILE=.env.www react-native run-android",
    "build-android-www": "cd android && cross-env ENVFILE=.env.www ./gradlew assembleRelease && cd ..",
    "ios": "cross-env ENVFILE=.env react-native run-ios",
    "lint": "eslint .",
    "lint-android-www": "cd android && cross-env ENVFILE=.env.www ./gradlew -Pci --console=plain :app:lintDebug -PbuildDir=lint",
    "start": "cross-env ENVFILE=.env react-native start",
    "start-www": "cross-env ENVFILE=.env.www react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-navigation/native": "^6.1.16",
    "@react-navigation/native-stack": "^6.9.25",
    "cross-env": "^7.0.3",
    "metro-react-native-babel-preset": "^0.77.0",
    "mobx": "^6.12.0",
    "mobx-react-lite": "^4.0.6",
    "moment": "^2.30.1",
    "react": "18.1.0",
    "react-native": "0.70.15",
    "react-native-base64": "^0.2.1",
    "react-native-config": "^1.5.1",
    "react-native-paper": "^5.12.3",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
